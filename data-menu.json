{
  "version": "1.0",
  "data_sources": [
    {
      "name": "binance_spot_price",
      "description": "Binance spot price for a symbol",
      "intent_patterns": [
        "spot",
        "price",
        "latest price"
      ],
      "method": "GET",
      "url": "https://api.binance.com/api/v3/ticker/price",
      "params": {
        "symbol": "BTCUSDT"
      },
      "headers": {},
      "response_path": ""
    },
    {
      "name": "binance_funding_rate_latest",
      "description": "Most recent funding rate from Binance perpetuals",
      "intent_patterns": [
        "funding rate",
        "perp funding"
      ],
      "method": "GET",
      "url": "https://fapi.binance.com/fapi/v1/fundingRate",
      "params": {
        "symbol": "BTCUSDT",
        "limit": "1"
      },
      "headers": {},
      "response_path": ""
    },
    {
      "name": "binance_open_interest",
      "description": "Current open interest on Binance perpetuals",
      "intent_patterns": [
        "open interest",
        "oi"
      ],
      "method": "GET",
      "url": "https://fapi.binance.com/fapi/v1/openInterest",
      "params": {
        "symbol": "BTCUSDT"
      },
      "headers": {},
      "response_path": ""
    },
    {
      "name": "coinglass_liquidation_history",
      "description": "CoinGlass aggregated liquidation history",
      "intent_patterns": [
        "liquidation",
        "liquidations",
        "liq"
      ],
      "method": "GET",
      "url": "https://open-api-v4.coinglass.com/api/futures/liquidation/history",
      "params": {
        "symbol": "BTC",
        "interval": "4h"
      },
      "headers": {
        "CG-API-KEY": "INLINE_CG_KEY"
      },
      "response_path": "data"
    },
    {
      "name": "coinglass_open_interest_history",
      "description": "CoinGlass open interest history",
      "intent_patterns": [
        "oi history",
        "open interest history"
      ],
      "method": "GET",
      "url": "https://open-api-v4.coinglass.com/api/futures/open-interest/history",
      "params": {
        "symbol": "BTCUSDT",
        "period": "4h"
      },
      "headers": {
        "CG-API-KEY": "INLINE_CG_KEY"
      },
      "response_path": "data"
    },
    {
      "name": "coinglass_funding_rate_history",
      "description": "CoinGlass funding rate history",
      "intent_patterns": [
        "funding rate history",
        "funding history"
      ],
      "method": "GET",
      "url": "https://open-api-v4.coinglass.com/api/futures/funding-rate/history",
      "params": {
        "symbol": "BTCUSDT",
        "period": "4h"
      },
      "headers": {
        "CG-API-KEY": "INLINE_CG_KEY"
      },
      "response_path": "data"
    },
    {
      "name": "coinglass_long_short_ratio_history",
      "description": "CoinGlass global long/short account ratio history",
      "intent_patterns": [
        "long short ratio",
        "lsr",
        "longs shorts"
      ],
      "method": "GET",
